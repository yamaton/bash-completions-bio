_datasets_summary_virus_genome_accession()
{
    last_command="datasets_summary_virus_genome_accession"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--inputfile=")
    two_word_flags+=("--inputfile")
    local_nonpersistent_flags+=("--inputfile")
    local_nonpersistent_flags+=("--inputfile=")
    flags+=("--annotated")
    flags+=("--api-key=")
    two_word_flags+=("--api-key")
    flags+=("--as-json-lines")
    flags+=("--complete-only")
    flags+=("--debug")
    flags+=("--geo-location=")
    two_word_flags+=("--geo-location")
    flags+=("--help")
    flags+=("--host=")
    two_word_flags+=("--host")
    flags+=("--limit=")
    two_word_flags+=("--limit")
    flags+=("--lineage=")
    two_word_flags+=("--lineage")
    flags+=("--refseq")
