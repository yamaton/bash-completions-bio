_sourmash_sketch()
{
    local cmd i subcommands
    local subcommands=" dna fromfile protein translate"

    for (( i=1; i < cword; i++ )); do
        if [[ " ${subcommands[*]} " == *" ${words[i]} "* ]]; then
            cmd=${words[i]}
            break
        fi
    done

    case "$cmd" in
        dna) _sourmash_sketch_dna
            return
            ;;

        fromfile) _sourmash_sketch_fromfile
            return
            ;;

        protein) _sourmash_sketch_protein
            return
            ;;

        translate) _sourmash_sketch_translate
            return
            ;;

    esac

    local word_list=" dna fromfile protein translate -h --help"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_sketch_dna()
{
    local word_list="  -h --help --license --check-sequence -p --param-string --from-file -f --force -o --output --set-name --name --merge --output-dir --outdir --singleton --name-from-first --randomize"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_sketch_fromfile()
{
    local word_list="  -h --help -p --param-string --already-done --license --check-sequence -o --output-signatures --force-output-already-exists --ignore-missing --output-csv-info --output-manifest-matching --report-duplicated"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_sketch_protein()
{
    local word_list="  -h --help --license -p --param-string --from-file -f --force -o --output --set-name --name --merge --output-dir --outdir --singleton --name-from-first --randomize --dayhoff --hp"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_sketch_translate()
{
    local word_list="  -h --help --license --check-sequence -p --param-string --from-file -f --force -o --output --set-name --name --merge --output-dir --outdir --singleton --name-from-first --randomize --dayhoff --hp"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_storage()
{
    local cmd i subcommands
    local subcommands=" convert"

    for (( i=1; i < cword; i++ )); do
        if [[ " ${subcommands[*]} " == *" ${words[i]} "* ]]; then
            cmd=${words[i]}
            break
        fi
    done

    case "$cmd" in
        convert) _sourmash_storage_convert
            return
            ;;

    esac

    local word_list=" convert -h --help"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_storage_convert()
{
    local word_list="  -h --help -b --backend"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_tax()
{
    local cmd i subcommands
    local subcommands=" annotate genome grep metagenome prepare summarize"

    for (( i=1; i < cword; i++ )); do
        if [[ " ${subcommands[*]} " == *" ${words[i]} "* ]]; then
            cmd=${words[i]}
            break
        fi
    done

    case "$cmd" in
        annotate) _sourmash_tax_annotate
            return
            ;;

        genome) _sourmash_tax_genome
            return
            ;;

        grep) _sourmash_tax_grep
            return
            ;;

        metagenome) _sourmash_tax_metagenome
            return
            ;;

        prepare) _sourmash_tax_prepare
            return
            ;;

        summarize) _sourmash_tax_summarize
            return
            ;;

    esac

    local word_list=" annotate genome grep metagenome prepare summarize -h --help"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_tax_annotate()
{
    local word_list="  -h --help -g --gather-csv --from-file -q --quiet -t --taxonomy-csv --taxonomy -o --output-dir --keep-full-identifiers --keep-identifier-versions --fail-on-missing-taxonomy -f --force --lins --lin-taxonomy --ictv --ictv-taxonomy"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_tax_genome()
{
    local word_list="  - -h --help -g --gather-csv --from-file -q --quiet -t --taxonomy-csv --taxonomy -o --output-base --output-dir --keep-full-identifiers --keep-identifier-versions --fail-on-missing-taxonomy -F --output-format -f --force --lins --lin-taxonomy --lingroup --lingroups --ictv --ictv-taxonomy --containment-threshold --ani-threshold --aai-threshold -r --rank --position --lin-position --v4 --v5"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_tax_grep()
{
    local word_list="  -h --help -r --rank -v --invert-match -i --ignore-case --silent --no-picklist-output -c --count -q --quiet -t --taxonomy-csv --taxonomy -o --output -f --force"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_tax_metagenome()
{
    local word_list="  -h --help -g --gather-csv --from-file -q --quiet -o --output-base --output-dir -t --taxonomy-csv --taxonomy --keep-full-identifiers --keep-identifier-versions --fail-on-missing-taxonomy -F --output-format -f --force --lins --lin-taxonomy --lingroup --lingroups --ictv --ictv-taxonomy -r --rank --position --lin-position --use-abundances --ignore-abundances --no-abundances --v4 --v5"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_tax_prepare()
{
    local word_list="  -h --help -q --quiet -t --taxonomy-csv --taxonomy -o --output -F --database-format --keep-full-identifiers --keep-identifier-versions --fail-on-missing-taxonomy -f --force"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_sourmash_tax_summarize()
{
    local word_list="  -h --help -q --quiet -o --output-lineage-information --keep-full-identifiers --keep-identifier-versions -f --force --lins --lin-taxonomy --ictv --ictv-taxonomy"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

## -o bashdefault and -o default are fallback
complete -o bashdefault -o default -F _sourmash sourmash
