        tax) _sourmash_tax
            return
            ;;

    esac

    local word_list=" compare compute gather index info plot prefetch search lca signature sketch storage tax -h --help -v --version -q --quiet"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

